<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>disable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Title>Qoph</Title>
    <Authors>Timwi</Authors>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <Description>Contains code used in the creation of the Quantum Obfuscation Puzzle Hunt (QOPH).</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsQuery" Version="1.3.4" />
    <PackageReference Include="PuzzleSolvers" Version="1.0.0" />
    <PackageReference Include="RT.Geometry" Version="2.0.1813" />
    <PackageReference Include="RT.Keyboard" Version="1.26.6" />
    <PackageReference Include="RT.Modeling" Version="1.7.0" />
    <PackageReference Include="RT.Serialization" Version="2.0.1813" />
    <PackageReference Include="RT.Serialization.Xml" Version="2.0.1813" />
    <PackageReference Include="RT.TagSoup" Version="1.1.52" />
    <PackageReference Include="RT.Util" Version="2.0.1813" />
    <PackageReference Include="RT.Util.Core" Version="2.0.1813" />
  </ItemGroup>

</Project>
